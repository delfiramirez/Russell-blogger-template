<?xml version="1.0" encoding="UTF-8"?>
<header role='banner'>
    <b:section class='header' id='header' maxwidgets='1' showaddelement='no'>
        <b:widget id='Header1' locked='true' mobile='yes' title='On Notation (Header)' type='Header'>
            <b:includable id='main'>
                <div class='header-frame'>
                    <b:include data='blog' name='title'/>
                </div>
            </b:includable>
            <b:includable id='description'>
                <p class='description'>
                    <data:description/>
                </p>
            </b:includable>
            <b:includable id='title' var='b'>
                <h1 itemprop='title'>
                    <a href='http://delfiramirez.blogspot.com' hreflang='en'>
                        <data:b.title/>
                    </a>
                </h1>
            </b:includable>
        </b:widget>
    </b:section>
</header>
